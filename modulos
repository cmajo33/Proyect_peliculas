    return Pelicula
#Pelicula5 = mod.crear_pelicula("Hombres de Negro", "Accion,Ciencia Ficcion", 98, 1997,'7+', 1340, "Martes")
Crear_Pelicula('Hombres de Negro','Accion/'  'Ciencia Ficcion Humoristica', 98,1997, '7+', 1340, 'Martes')

def encontrar_pelicula(nombre_pelicula: str, p1: dict, p2: dict, p3: dict, p4: dict,  p5: dict) -> dict:
    """Encuentra en cual de los 5 diccionarios que se pasan por parametro esta la
       pelicula cuyo nombre es dado por parametro.
       Si no se encuentra la pelicula se debe retornar None.
    Parametros:
        nombre_pelicula (str): El nombre de la pelicula que se desea encontrar.
        p1 (dict): Diccionario que contiene la informacion de la pelicula 1.
        p2 (dict): Diccionario que contiene la informacion de la pelicula 2.
        p3 (dict): Diccionario que contiene la informacion de la pelicula 3.
        p4 (dict): Diccionario que contiene la informacion de la pelicula 4.
        p5 (dict): Diccionario que contiene la informacion de la pelicula 5.
    Retorna:
        dict: Diccionario de la pelicula cuyo nombre fue dado por parametro.
        None si no se encuentra una pelicula con ese nombre.
    """
    #TODO: completar y remplazar la siguiente línea por el resultado correcto
    lista_peliculas=[p1['Volver al Futuro'],p2['Jumanji'],p3['El Conjuro'], p4['Trampa en alta mar'], p5['Hombres de Negro']]
    for i in lista_peliculas:
      if i==nombre_pelicula:
        return i
    return d['nombre_pelicula']

    return None

def encontrar_pelicula(nombre_pelicula: str, p1: dict, p2: dict, p3: dict, p4: dict,  p5: dict) -> dict:
    """Encuentra en cual de los 5 diccionarios que se pasan por parametro esta la
       pelicula cuyo nombre es dado por parametro.
       Si no se encuentra la pelicula se debe retornar None.
    Parametros:
        nombre_pelicula (str): El nombre de la pelicula que se desea encontrar.
        p1 (dict): Diccionario que contiene la informacion de la pelicula 1.
        p2 (dict): Diccionario que contiene la informacion de la pelicula 2.
        p3 (dict): Diccionario que contiene la informacion de la pelicula 3.
        p4 (dict): Diccionario que contiene la informacion de la pelicula 4.
        p5 (dict): Diccionario que contiene la informacion de la pelicula 5.
    Retorna:
        dict: Diccionario de la pelicula cuyo nombre fue dado por parametro.
        None si no se encuentra una pelicula con ese nombre.
    """
    #TODO: completar y remplazar la siguiente línea por el resultado correcto
    d={p1['Vover al Futuro']:p1, p2['Jumanji']:p2, p3['El Conjuro']:p3, p4['Trampa en alta mar']:p4, p5['Hombres de Negro']:p5}
    #d['Volver al Futuro']
    return d[nombre_pelicula]

# Create dictionary variables for movies p1 through p5
p1 = {'Vover al Futuro': 'Volver al Futuro'}
p2 = {'Jumanji': 'Jumanji'}
p3 = {'El Conjuro': 'El Conjuro'}
p4 = {'Trampa en alta mar': 'Trampa en alta mar'}
p5 = {'Hombres de Negro': 'Hombres de Negro'}

encontrar_pelicula('Hombres de Negro', p1, p2, p3, p4, p5)

key='p2'
l=['p1', 'p2', 'p3','p4', 'p5']
for i in l:
  if i==key:
    print(i)
  else:
    print('NO encontrado')
